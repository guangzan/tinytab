{
  "name": "TinyTab",
  "description": "简洁高效的新标签页",
  "version": "1.1.1",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": "images/logo16.png",
    "default_title": "TinyTab"
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  }
}
